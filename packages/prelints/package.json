{
  "name": "@yirga/prelints",
  "version": "0.2.1",
  "description": "prettier & lints",
  "author": "xyuanbuilds <1076849402@qq.com>",
  "homepage": "https://github.com/xyuanbuilds/yirga#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./eslint": "./lib/eslint.js",
    "./prettier": "./lib/prettier.js",
    "./stylelint": "./lib/stylelint.js",
    "./tsconfig.default.json": "./lib/tsconfig.default.json",
    "./tsconfig.dev.json": "./lib/tsconfig.dev.json",
    "./lib/": "./lib/"
  },
  "module": "es/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "es"
  ],
  "keywords": [
    "yirga",
    "eslint",
    "prettier",
    "stylelint"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyuanbuilds/yirga"
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "build:es": "tsc --build ./tsconfig.build.es.json",
    "deps": "yarn upgrade-interactive --latest",
    "clean": "tsc -b tsconfig.build.json --clean",
    "clean:es": "tsc -b tsconfig.build.es.json --clean"
  },
  "dependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@tsconfig/node14": "^1.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "debug": "^4.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^22.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^14.11.1",
    "debug": "^4.2.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@rushstack/eslint-patch": "^1.0.3"
  }
}
